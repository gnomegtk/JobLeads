{% extends 'base.html.twig' %}

{% block title %}County{% endblock %}

{% block body %}
    <h1>County</h1>

    <table class="table">
        <tbody>
            <tr>
                <th scope="col">#</th>
                <td>{{ county.id }}</td>
            </tr>
            <tr>
                <th scope="col">Country</th>
                <td>{{ county.state.country.name }}</td>
            </tr>
            <tr>
                <th scope="col">State</th>
                <td>{{ county.state.name }}</td>
            </tr>
            <tr>
                <th scope="col">Name</th>
